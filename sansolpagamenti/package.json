{
  "name": "la-mia-salute",
  "version": "0.30.0",
  "description": "Piattaforma web dedicata al cittadino che integra tutti i servizi sanitari messi a disposizione dalla regione Piemonte",
  "productName": "Salute Piemonte",
  "cordovaId": "it.csi.apps.lamiasalute",
  "author": "",
  "private": true,
  "scripts": {
    "dev:spa": "cross-env BUILD_ENVIRONMENT=development quasar dev",
    "dev:pwa": "cross-env BUILD_ENVIRONMENT=development quasar dev -m pwa",
    "build:test:spa": "cross-env BUILD_ENVIRONMENT=test quasar build -c -m spa --debug",
    "build:test:pwa": "cross-env BUILD_ENVIRONMENT=test quasar build -c -m pwa --debug",
    "build:test-cert:pwa": "cross-env BUILD_ENVIRONMENT=test-cert quasar build -c -m pwa --debug",
    "build:test-nivola:pwa": "cross-env BUILD_ENVIRONMENT=test-nivola quasar build -c -m pwa --debug",
    "build:test-cert-nivola:pwa": "cross-env BUILD_ENVIRONMENT=test-cert-nivola quasar build -c -m pwa --debug",
    "build:alpha:pwa": "cross-env BUILD_ENVIRONMENT=alpha quasar build -c -m pwa",
    "build:alpha-cert:pwa": "cross-env BUILD_ENVIRONMENT=alpha-cert quasar build -c -m pwa",
    "build:production:pwa": "cross-env BUILD_ENVIRONMENT=production quasar build -c -m pwa",
    "bom": "npx npm-license-crawler --onlyDirectDependencies --prooduction --csv bom.csv --yes && node bom-cleaner.js"
  },
  "dependencies": {
    "@chenfengyuan/vue-qrcode": "^1.0.2",
    "@firebase/app": "^0.3.5",
    "@firebase/messaging": "^0.3.7",
    "axios": "^0.18.1",
    "axios-mock-adapter": "^1.16.0",
    "blueimp-md5": "^2.10.0",
    "change-case": "^4.1.1",
    "date-fns": "^1.30.1",
    "file-saver": "^2.0.0",
    "iframe-resizer": "^4.2.10",
    "jsbarcode": "^3.11.0",
    "leaflet": "^1.6.0",
    "portal-vue": "^1.3.0",
    "quagga": "^0.12.1",
    "rss-parser": "^3.7.5",
    "vue-browser-geolocation": "^1.8.0",
    "vue-i18n": "^8.0.0",
    "vue-json-csv": "^1.2.12",
    "vue-matomo": "^3.14.0-0",
    "vue-qrcode-reader": "^0.7.3",
    "vue-sticky-directive": "0.0.10",
    "vue2-leaflet": "^2.5.2",
    "vue2-leaflet-markercluster": "^3.1.0",
    "vuelidate": "^0.7.4"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "quasar-cli": "^0.17.26",
    "strip-ansi": "^3.0.1"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
